<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mms.baklava7" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Deneme</name>
    <description>
        Deneme.De Mobil UygulamasÄ±
    </description>
    <author email="info@kivancekici.com" href="http://kivancekici.com">
        M.Sc. Computer Engineer Suleyman Kivanc Ekici
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="images/navbarlogo.png" platform="android" qualifier="ldpi" />
    <icon src="images/navbarlogo.png" platform="android" qualifier="mdpi" />
    <icon src="images/navbarlogo.png" platform="android" qualifier="hdpi" />
    <icon src="images/navbarlogo.png" platform="android" qualifier="xhdpi" />
    <icon src="images/navbarlogo.png" platform="android" qualifier="xxhdpi" />
    <icon src="images/navbarlogo.png" platform="android" qualifier="xxxhdpi" />
    <icon src="images/navbarlogo.png" platform="android" qualifier="fr-xxhdpi" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
</widget>
